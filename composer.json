{
    "name": "survos/datatables-bundle",
    "description": "incorporate DatatablesNet, using twig and stimulus",
    "keywords": ["symfony-ux", "symfony6"],

    "type": "symfony-bundle",
    "license": "MIT",

    "authors": [
        {
            "name": "Tac Tacelosky",
            "email": "tacman@gmail.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "api-platform/core": "^2.6",
        "friendsofsymfony/jsrouting-bundle": "^3.1",
        "symfony/config": "^6.1",
        "symfony/dependency-injection": "^6.1",
        "symfony/http-kernel": "^6.1",
        "symfony/ux-twig-component": "^2.1",
        "twig/twig": "^2 || ^3.4"
    },
    "autoload": {
        "psr-4": {
            "Survos\\Datatables\\": "src/"
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^1.7",
        "symfony/browser-kit": "^6.1",
        "symfony/framework-bundle": "^6.1",
        "symfony/phpunit-bridge": "^6.1",
        "symfony/twig-bundle": "^6.1",
        "symfony/var-dumper": "^6.1",
        "symfony/webpack-encore-bundle": "^1.11"
    }


}
